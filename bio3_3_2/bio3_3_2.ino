
int k=5;

void setup() {
  pinMode(k,OUTPUT);
}

void loop() {
  digitalWrite(k,LOW);
  delay(2);
  digitalWrite(k,HIGH);
  delay(2);
  digitalWrite(k,HIGH);
  delay(2);
  digitalWrite(k,LOW);
  delay(2);
  digitalWrite(k,LOW);
  delay(2);
  digitalWrite(k,HIGH);
  delay(2);
  digitalWrite(k,LOW);
  delay(2);
  digitalWrite(k,HIGH);
  delay(2);
  digitalWrite(k,LOW);
  delay(2);
  digitalWrite(k,LOW);
  delay(2);
  digitalWrite(k,HIGH);
  delay(2);
  digitalWrite(k,HIGH);
  delay(2);
}
